{
  "name": "afinia-ingest",
  "version": "0.1.0",
  "private": true,
  "exports": {
    "./schema": "./src/db/schema.ts"
  },
  "scripts": {
    "db": "sst shell drizzle-kit",
    "db:seed": "sst shell -- tsx --env-file=.env ./src/provider/up/modules/loadData.ts",
    "db:generate": "sst shell drizzle-kit generate",
    "db:migrate": "sst shell drizzle-kit migrate",
    "dev": "sst secret load ./.env && sst dev",
    "load-env": "sst secret load ./.env",
    "sso": "aws sso login --sso-session=soppro"
  },
  "dependencies": {
    "afinia-common": "workspace:*",
    "drizzle-orm": "^0.44.6",
    "openapi-fetch": "^0.15.0",
    "postgres": "^3.4.7",
    "sst": "3.17.19"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.155",
    "@types/node": "^20",
    "drizzle-kit": "^0.31.5",
    "tsx": "^4.20.6"
  }
}