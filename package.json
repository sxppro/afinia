{
  "name": "afinia",
  "version": "0.1.0",
  "engines": {
    "pnpm": ">=8"
  },
  "scripts": {
    "db:seed": "pnpm --filter afinia-ingest db:seed",
    "db:generate": "pnpm --filter afinia-ingest db:generate",
    "db:migrate": "pnpm --filter afinia-ingest db:migrate",
    "common:generate:types": "pnpm --filter afinia-common generate:types",
    "sso": "pnpm --filter afinia-ingest sso",
    "web:dev": "pnpm --filter afinia-web dev"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}